package _20210711;

public class me {

	public static long main(String[] args) {
		long n = 121;
		// long n = 3;
		
		long answer = 0;
        
        for(answer = 1; answer <= n; answer++){
            if(answer*answer == n)
                return (answer+1)*(answer+1);
        }
        return -1;
	}

}
